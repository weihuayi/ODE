    "collapsed": false
   },
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "g(x)*D[0](f)(x)/f(x)^2 - D[0](g)(x)/f(x)\n",
      "g(x)*D[0](f)(x)/f(x)^2 - D[0](g)(x)/f(x)\n"
     ]
    }
   ],
   "source": [
    "var('x')\n",
    "g= function ('g')(x)\n",
    "f= function ('f')(x)\n",
    "y=-g(x)/f(x)\n",
    "a=diff(y,x)\n",
    "print(a)\n",
    "b=diff(f)\n",
    "c=diff(g)\n",
    "d=b*y^2/g-c/f\n",
    "print(d)"
   ]
  },
